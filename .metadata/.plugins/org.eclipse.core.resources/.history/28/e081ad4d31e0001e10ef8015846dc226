<script>
    jQuery(document).ready(function() {
        jQuery('.no_benefit').on("click", function(e) {
            jQuery('#registerPopup').addClass("clear");
        });

        jQuery('.yes_benefit').on("click", function(e) {
            var asModel = jQuery('#asModel').val();
            var asSerial = jQuery('#asSerial').val();
            var userPhone = jQuery('#userPhone').val();
            var userEmail = jQuery('#userEmail').val();
            var buyYear = jQuery('#sb_ref_year').val();
            var buyMonth = jQuery('#sb_ref_month').val();
            var marketList = jQuery('#marketList').val();

            var userInfo = {
                "model" : asModel,
                "serial" : asSerial,
                "phone" : userPhone,
                "email" : userEmail,
                "buyYear" : buyYear,
                "buyMonth" : buyMonth,
                "market" : marketList,
            };
            localStorage.setItem("userInfo", JSON.stringify(userInfo));
            window.open('/register/?pageNum=1');

            jQuery('#registerPopup').addClass("clear");
        });
    });
</script>
