<script>
    jQuery(document).ready(function() {
        var pageNum;
        var nowURL = location.href;

        const urlParams = new URL(location.href).searchParams;
        pageNum = urlParams.get('pageNum');
        if (!pageNum) {
            pageNum = 0;
        }else if (pageNum == 0) {
            jQuery("#terms").css("display", "block");
            jQuery("#asPage").css("display", "none");
        } else {
            jQuery("#terms").css("display", "none");
            jQuery("#asPage").css("display", "block");
            if(pageNum == 1) {
                jQuery('.page1').css("display", "block");
                jQuery('.page2').css("display", "none");
            }
            /* 
            else if (pageNum == 2) {
                jQuery('.page1').css("display", "none");
                jQuery('.page2').css("display", "block");
            }
            */
        }
    });
</script>
