</style>
<script>
    jQuery(document).ready(function() {
        jQuery('.search_btn').on("click", function(e) {
            jQuery('#search_popup').removeClass("clear");
        });

        jQuery('#search_popup_close').on("click", function(e) {
            jQuery('#search_popup').addClass("clear");
        });

        jQuery('.btn_search').on("click", function(e) {
            var model = jQuery("#model").val();
            var serial = jQuery("#serial").val();
            var userPhone = jQuery("#searchForm #user_phone").val().replaceAll(" ", "").replaceAll("-", "");

            if (!model.trim()) {
                jQuery("#searchModelErrMsg").css("display", "block");
                jQuery("#searchSerialErrMsg").css("display", "none");
                jQuery("#searchPhoneErrMsg").css("display", "none");
                jQuery("#searchForm #model").select();
            }
            else if (!serial.trim()) {
                jQuery("#searchModelErrMsg").css("display", "none");
                jQuery("#searchSerialErrMsg").css("display", "block");
                jQuery("#searchPhoneErrMsg").css("display", "none");
                jQuery("#searchForm #serial").select();
            }
            else if (!userPhone.trim()) {
                jQuery("#searchModelErrMsg").css("display", "none");
                jQuery("#searchSerialErrMsg").css("display", "none");
                jQuery("#searchPhoneErrMsg").css("display", "block");
                jQuery("#searchForm #user_phone").select();
            }
            else {
                var formData = new FormData(jQuery('#searchForm')[0]);
                formData.append("user_phone", userPhone);

                jQuery.ajax({
                    url: '//admin.tbnws.com/subscribe/search',
                    type: 'POST',
                    data: formData,
                    async: false,
                    processData: false,
                    contentType: false,
                    dataType: 'json',
                })
                    .success(function (result) {


                        if (!result.serial) { // 시리얼 자체가 없음
                            alert("해당 시리얼넘버로 정품등록이 확인되지 않습니다.")
                        }
                        else if (!result.user_phone) {	// 정품등록 정보 없을 경우
                            alert("해당 시리얼넘버로 정품등록이 확인되지 않습니다..")
                        }
                        else {	// 정품등록 정보 있을 경우
                            jQuery('#search_popup').addClass("clear");

                            var buyDate = result.buy_date.split('-');

                            jQuery("#asModel").val(model);
                            jQuery("#asSerial").val(result.serial);
                            jQuery("#userPhone").val(result.user_phone);
                            jQuery("#userEmail").val(result.user_email);

                            jQuery("#asModel").attr('readonly', true);
                            jQuery("#asSerial").attr('readonly', true);
                            jQuery("#userPhone").attr('readonly', true);
                            jQuery("#userEmail").attr('readonly', true);
                            
                            jQuery("#sb_ref_year").val(buyDate[0]).trigger('change');
                            jQuery("#sb_ref_month").val(buyDate[1]).trigger('change');
                            jQuery("#marketList").val(result.market).trigger('change');

                            jQuery("#sb_ref_year").attr('disabled', true);
                            jQuery("#sb_ref_month").attr('disabled', true);
                            jQuery("#marketList").attr('disabled', true);
                        }
                        
                    })
                    .error(function (jqXHR, textStatus, errorThrown) {
                        alert("해당 시리얼넘버로 정품등록이 확인되지 않습니다.");

                        console.log(jqXHR.responseText);
                        console.log(textStatus);
                        console.log(errorThrown);
                    });
            }
        });
    });
</script>

