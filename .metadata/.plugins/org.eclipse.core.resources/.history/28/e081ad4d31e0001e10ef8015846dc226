        margin-top: 10px;
        display: inline-block;
        font-weight: 300;
        color: #6E6E6E;
    }
    #asForm #uploadFile .filebox .delete{
        color: #ff5353;
        margin-left: 5px;
        cursor: pointer;
    }

    .dialogWide > div {
        margin-top: 5% !important;
        max-width: 80% !important;
