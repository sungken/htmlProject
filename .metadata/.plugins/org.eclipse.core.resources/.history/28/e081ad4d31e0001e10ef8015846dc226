<script>
    var fileNo = 0;
    var filesArr = new Array();

    function addFile(obj){
        var maxFileCnt = 5;   // 첨부파일 최대 개수
        var attFileCnt = document.querySelectorAll('.filebox').length;    // 기존 추가된 첨부파일 개수
        var remainFileCnt = maxFileCnt - attFileCnt;    // 추가로 첨부가능한 개수
        var curFileCnt = obj.files.length;  // 현재 선택된 첨부파일 개수

        if (curFileCnt > 0) {
            jQuery('#uploadFile').css("display", "block");
        } else {
            jQuery('#uploadFile').css("display", "none");
        }

        // 첨부파일 개수 확인
        if (curFileCnt > remainFileCnt) {
            alert("첨부파일은 최대 " + maxFileCnt + "개 까지 첨부 가능합니다.");
        } else {
            for (const file of obj.files) {
                var fSExt = new Array('Bytes', 'KB', 'MB', 'GB');
                var i = 0;
                var checkSize = file.size;
                
                while (checkSize > 900) {
                
                    checkSize /= 1024;
                    i++;
                    
                }
                checkSize = (Math.round(checkSize * 100) / 100) + ' ' + fSExt[i];

                // 첨부파일 검증
                if (validation(file)) {
                    // 파일 배열에 담기
                    var reader = new FileReader();
                    reader.onload = function () {
                        filesArr.push(file);
                    };
                    reader.readAsDataURL(file);

                    // 목록 추가
                    let htmlData = '';
                    htmlData += '<div id="file' + fileNo + '" class="filebox">';
                    htmlData += '   <p class="name">' + file.name + '</p>';
                    htmlData += '<div style="float: right;">'
                    htmlData += '   <p class="fileSize">' + checkSize + '</p>';
                    htmlData += '   <a class="delete" onclick="deleteFile(' + fileNo + ');">';
                    htmlData += '    <svg xmlns="http://www.w3.org/2000/svg" width="10.121" height="10.121" viewBox="0 0 10.121 10.121">';
                    htmlData += '        <g id="그룹_192" data-name="그룹 192" transform="translate(-1172.938 -969.938)">';
                    htmlData += '          <path id="패스_6241" data-name="패스 6241" d="M0,0,5.5,5.5,8,8" transform="translate(1173.999 970.999)" fill="rgba(0,0,0,0)" stroke="#f22e22" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5"/>';
                    htmlData += '          <line id="선_33" data-name="선 33" x1="8" y2="8" transform="translate(1173.999 970.999)" fill="none" stroke="#f22e22" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5"/>';
                    htmlData += '        </g>';
                    htmlData += '      </svg>';
                    htmlData += '</a>';
                    htmlData += '</div>'
                    htmlData += '</div>';
                    jQuery('#uploadFile').append(htmlData);
                    fileNo++;
                } else {
                    continue;
                }
            }
        }
        // 초기화
        document.querySelector("input[type=file]").value = "";
    }

    /* 첨부파일 검증 */
    function validation(obj){
        const fileTypes = ['application/pdf', 'image/gif', 'image/jpeg', 'image/png', 'image/bmp', 'image/tif', 'video/mp4', 'video/mkv', 'video/quicktime', 'application/haansofthwp', 'application/x-hwp'];
        if (obj.name.length > 100) {
            alert("파일명이 100자 이상인 파일은 제외되었습니다.");
            return false;
        } else if (obj.size > (100 * 1024 * 1024)) {
            alert("최대 파일 용량인 100MB를 초과한 파일은 제외되었습니다.");
            return false;
        } else if (obj.name.lastIndexOf('.') == -1) {
            alert("확장자가 없는 파일은 제외되었습니다.");
            return false;
        } else if (!fileTypes.includes(obj.type)) {
            alert("첨부가 불가능한 파일은 제외되었습니다.");
            return false;
        } else {
            return true;
        }
    }

    /* 첨부파일 삭제 */
    function deleteFile(num) {
        document.querySelector("#file" + num).remove();
        filesArr[num].is_delete = true;
    }

    function chkPhone(str) {
        str = str.replace(/-/gi,'');    // - 제거
        str = str.replace(/\./gi,'');    // . 제거
        str = str.replace(/ /gi,'');    // 공백 제거

        var regExp =/(01[016789])([1-9]{1}[0-9]{2,3})([0-9]{4})$/;
        var myArray;

        if(regExp.test(str)){
            myArray = regExp.exec(str);
            // console.log(myArray[1]);
            // console.log(myArray[2]);
            // console.log(myArray[3]);
            return true;
        }
        else {
            return false;
        }
    }

    function chkEmail(str) {
        var regExp = /^[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*.[a-zA-Z]{2,3}$/i;
        if (regExp.test(str)) return true;
        else return false;
    }

    // 200402. KimGoon. 조건 추가
    // 191115. KimGoon. 모델명 유효성 추가
    function chkModel(str) {
        var regExp = /K[A-H,J,L-M][1-4]{1,2}$/i;
        if (regExp.test(str)) {
            return true;
        }
        else {
            regExp = /[A][3][P]$/i;
            if (regExp.test(str)) {
                return true;
            }
            else {
                return false;
            }
        }
    }

    jQuery(document).ready(function() {
        var nowURL = location.href;

        let today = new Date(); 
        let year = today.getFullYear();

        var year_option = '';

        for (year; year > 2017; year--) {
            year_option += '<option value="' + (year) + '">' + (year) + '년</option>'; 
        }

        jQuery('#sb_ref_year').html(year_option);

        jQuery('#userAddress').on("click", function() {
            new daum.Postcode({
                oncomplete: function(data) {
                    document.getElementById('userAddress').value = data.address; // 주소 넣기
                    document.querySelector("#userAddress2").focus(); //상세입력 포커싱
                }
            }).open();
        });

        jQuery('#addressSearch').on("click", function() {
            new daum.Postcode({
                oncomplete: function(data) {
                    document.getElementById('userAddress').value = data.address; // 주소 넣기
                    document.querySelector("#userAddress2").focus(); //상세입력 포커싱
                }
            }).open();
        });

        jQuery('.btn_next').on("click", function() {
            var asSerial = jQuery('#asSerial').val();
            var asModel = jQuery('#asModel').val();
            var asText = jQuery('#asText').val();

            if (!asSerial.trim()) {
                alert("시리얼를 입력해주세요");
                jQuery("#asSerial").focus().select();
            }
            else if (!asModel.trim()) {
                alert("모델명를 입력해주세요");
                jQuery("#asModel").focus().select();
            }
            else if (!asText.trim()) {
                alert("문의내용를 입력해주세요");
                jQuery("#asText").focus().select();
            } else {
                jQuery('.page1').css("display", "none");
                jQuery('.page2').css("display", "block");
            }
        });

        jQuery('.btn_back').on("click", function() {
            jQuery('.page1').css("display", "block");
            jQuery('.page2').css("display", "none");
        });

        jQuery('.btn_end').on("click", function() {
            var asSerial = jQuery('#asSerial').val();
            var asModel = jQuery('#asModel').val();
            var asText = jQuery('#asText').val();
            var marketList = jQuery('#marketList').val();
            var osList = jQuery('#osList').val();
            var buyYear = jQuery('#sb_ref_year').val();
            var buyMonth = jQuery('#sb_ref_month').val();
            var userName = jQuery('#userName').val();
            var userPhone = jQuery('#userPhone').val();
            var userEmail = jQuery('#userEmail').val();
            var userAddress = jQuery('#userAddress').val();
            var userAddress2 = jQuery('#userAddress2').val();

            if (!userName.trim()) {
                alert("이름를 입력해주세요");
                jQuery("#userName").focus().select();
            }
            else if (!userPhone.trim()) {
                alert("전화번호를 입력해주세요");
                jQuery("#userPhone").focus().select();
            }
            else if (!chkPhone(userPhone)) {
                alert("전화번호 양식을 확인해주세요.");
                jQuery("#userPhone").focus().select();
            }
            else if (!userEmail.trim()) {
                alert("이메일을 입력해주세요");
                jQuery("#userEmail").focus().select();
            }
            else if (!chkEmail(userEmail)) {
                alert("이메일 양식을 확인해주세요");
                jQuery("#userEmail").focus().select();
            } 
            else if (!userAddress.trim()) {
                alert("주소를 확인해주세요");
                jQuery("#userAddress").focus().select();
            } 
            else if (!userAddress2.trim()) {
                alert("상세주소를 확인해주세요");
                jQuery("#userAddress2").focus().select();
            } else {
                var today = new Date();   

                var year = today.getFullYear();
                var month = today.getMonth() + 1;
                var date = today.getDate();

                jQuery('#endUserName').text(userName);
                jQuery('#endRegisterDate').text((year + '.' + month + '.' + date));
                jQuery('#endMarket').text(marketList);
                jQuery('#endBuyDate').text(buyYear + '.' + buyMonth);
                jQuery('#endModel').text(asModel);
                jQuery('#endAsText').text(asText);

                var formData = new FormData();
                formData.append("from", "Keychron");
                formData.append("type", "A/S");
                formData.append("as_name", userName);
                formData.append("as_phone", userPhone);
                formData.append("as_email", userEmail);
                formData.append("as_address", userAddress + " " + userAddress2);
                formData.append("brand-select", "360006154672");
                formData.append("as_product_name", asModel);
                formData.append("as_serial_number", asSerial);
                formData.append("as_buy_address", marketList);
                formData.append("user_environment", osList);
                formData.append("buy_date_year", buyYear);
                formData.append("buy_date_month", buyMonth);
                formData.append("as_contents", asText);

                for (var i = 0; i < filesArr.length; i++) {
                    // 삭제되지 않은 파일만 폼데이터에 담기
                    if (!filesArr[i].is_delete) {
                        formData.append("inputFile", filesArr[i]);
                    }
                }

                jQuery.ajax({
                    //url: '//www.sidebyside.co.kr/upload/forum',
                    url: '//admin.tbnws.com/upload/forum',
                    type: 'POST',
                    data: formData,
                    async: false,
                    processData: false,
                    contentType: false,
                    dataType: 'json',
                    //dataType: 'jsonp',
                }).success(function (result1) {
                    if (result1.rtn == 'success') {
                        formData.append('fileList', result1.fileList);
                    }
                    else {
                        alert("파일 업로드 오류. 다시 한 번 시도 부탁드립니다. : " + result1);
                    }
                }).error(function (jqXHR, textStatus, errorThrown) {
                    console.log(jqXHR.responseText);
                    console.log(textStatus);
                    console.log(errorThrown);
                });

                jQuery.ajax({
                    url: '//support.gtgear.co.kr/zendesk',
                    //url: 'http://localhost:8082/zendesk',
                    type: 'POST',
                    data: formData,
                    async: false,
                    processData: false,
                    contentType: false,
                    //dataType: 'jsonp',
                }).success(function (result2) {
                    if (result2 == 'success') {
                        jQuery('#asPage').css("display", "none");
                        jQuery('#asEnd').css("display", "block");
                        var formData1 = new FormData();

                        formData1.append("model", asModel)
                        formData1.append("serial", asSerial)

                        jQuery.ajax({
                            url: '//admin.tbnws.com/subscribe/search',
                            type: 'POST',
                            data: formData1,
                            async: false,
                            processData: false,
                            contentType: false,
                            dataType: 'json',
                        })
                        .success(function (result) {
                            if (!result) {	// 정품등록 정보 없을 경우
                                jQuery('#registerPopup').removeClass("clear");
                            }
                            else {	// 정품등록 정보 있을 경우
                                
                            }
                            
                        })
                        .error(function (jqXHR, textStatus, errorThrown) {
                            jQuery('#registerPopup').removeClass("clear");
                            console.log(jqXHR.responseText);
                            console.log(textStatus);
                            console.log(errorThrown);
                        });
                    }
                    else if (result2 == 'false') {
                        if (!confirm("동일한 내용이 이미 등록되었습니다. 계속 등록하시겠습니까?")) {
                            location.reload();
                        } else {
                            formData.append('flag', 'Y');
                            jQuery.ajax({
                                url: '//support.gtgear.co.kr/zendesk',
                                //url: 'http://localhost:8082/zendesk',
                                type: 'POST',
                                data: formData,
                                async: false,
                                processData: false,
                                contentType: false,
                            }).success(function (result2) {
                                if (result2 == 'success') {
                                    jQuery('#asPage').css("display", "none");
                                    jQuery('#asEnd').css("display", "block");
                                    var formData1 = new FormData();

                                    formData1.append("model", asModel)
                                    formData1.append("serial", asSerial)

                                    jQuery.ajax({
                                        url: '//admin.tbnws.com/subscribe/search',
                                        type: 'POST',
                                        data: formData1,
                                        async: false,
                                        processData: false,
                                        contentType: false,
                                        dataType: 'json',
                                    })
                                    .success(function (result) {
                                        if (!result) {	// 정품등록 정보 없을 경우
                                            jQuery('#registerPopup').removeClass("clear");
                                        }
                                        else {	// 정품등록 정보 있을 경우
                                            
                                        }
                                        
                                    })
                                    .error(function (jqXHR, textStatus, errorThrown) {
                                        jQuery('#registerPopup').removeClass("clear");
                                        console.log(jqXHR.responseText);
                                        console.log(textStatus);
                                        console.log(errorThrown);
                                    });
                                }
                                else {
                                    alert("오류가 발생하였습니다. 다시 한 번 시도 부탁드립니다. : " + result2);
                                }
                            }).error(function (jqXHR, textStatus, errorThrown) {
                                console.log(jqXHR.responseText);
                                console.log(textStatus);
                                console.log(errorThrown);
                            });
                        }
                    }
                    else {
                        alert("오류가 발생하였습니다. 다시 한 번 시도 부탁드립니다. : " + result2);
                    }
                }).error(function (jqXHR, textStatus, errorThrown) {
                    console.log(jqXHR.responseText);
                    console.log(textStatus);
                    console.log(errorThrown);
                });
            }
        });
    });
</script>

